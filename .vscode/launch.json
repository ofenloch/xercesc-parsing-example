    "version": "0.2.0",
    "configurations": [
        {
            "name": "parse people.xml",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/xercesc-parsing-examplebin",
            "args": ["${workspaceFolder}/data/people.xml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "parse people-with-errors.xml",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/xercesc-parsing-examplebin",
            "args": ["${workspaceFolder}/data/people-with-errors.xml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}